<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Vanilla Factions Expanded - Medieval</li>
        </mods>
        <match Class="PatchOperationSequence">
            <success>Always</success>
            <operations>
                <li Class="PatchOperationAdd">
                    <xpath>/Defs/ItemProcessor.CombinationDef [defName = "VFEM_SmokerMeatToSmokedMeat"]/disallowedThingDefs</xpath>
                    <value>
                        <li>SM_SaltedMeat</li>
                    </value>
                </li>
                <li Class="PatchOperationAdd">
                    <xpath>/Defs/ItemProcessor.CombinationDef [defName = "SM_MeatIntoSaltedMeat"]/disallowedThingDefs</xpath>
                    <value>
                        <li>VFEM_SmokedMeat</li>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>
</Patch>